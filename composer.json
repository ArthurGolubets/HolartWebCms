{
    "name": "holartweb/holart-cms",
    "description": "Admin panel for Laravel 12 with Vue.js",
    "version": "1.0.2.2",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "admin", "cms", "vue"],
    "authors": [
        {
            "name": "HolartWeb",
            "email": "info@holartweb.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "HolartWeb\\HolartCMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HolartWeb\\HolartCMS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HolartWeb\\HolartCMS\\HolartCMSServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
